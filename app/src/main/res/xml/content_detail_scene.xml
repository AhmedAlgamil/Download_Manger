<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    motion:defaultDuration="1000"
    >

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        android:id="@+id/myTransacton"
        motion:autoTransition="animateToEnd"
        >
       <KeyFrameSet>


       </KeyFrameSet>
        <OnClick motion:clickAction="toggle"
            motion:targetId="@+id/btn_back"/>

    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.16"
            android:alpha="0.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.079" />
        <Constraint
            android:id="@+id/textView_file_name"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.243"
            android:alpha="0.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.077"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.334"
            android:alpha="0.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.072" />
        <Constraint
            android:id="@+id/textView_status"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.452"
            android:alpha="0.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.077"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            motion:layout_constraintVertical_bias="0.073"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.079"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:alpha="1.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            android:id="@+id/textView" />
        <Constraint
            motion:layout_constraintVertical_bias="0.149"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.077"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:alpha="1.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            android:id="@+id/textView_file_name" />
        <Constraint
            motion:layout_constraintVertical_bias="0.227"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.072"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:alpha="1.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            android:id="@+id/textView2" />
        <Constraint
            motion:layout_constraintVertical_bias="0.316"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.077"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:alpha="1.0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            android:id="@+id/textView_status" />
    </ConstraintSet>
</MotionScene>